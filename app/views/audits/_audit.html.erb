<tr>
  <td></td>
  <td>
    <%= l audit.created_at %>
  </td>
  <td>
    <%= time_ago_in_words(audit.created_at) %>
  </td>
  <td width="23%">
    <%= audit.try(:user).try(:email) %>
  </td>
  <td>
    <%= audit.auditable_type %>
  </td>
  <td>
    <%= audit.auditable_id %>
  </td>
  <td>
    <%= audit.action.humanize %>
  </td>
  <td>
    <%= sanitize prettify(audit).join('<br>') %>
  </td>
</tr>
